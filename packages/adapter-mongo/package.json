{
  "name": "@testurio/adapter-mongo",
  "version": "0.1.0",
  "description": "MongoDB adapter for Testurio DataSource",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check . --fix"
  },
  "dependencies": {
    "mongodb": "^6.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^24.10.0",
    "testurio": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "testurio": "workspace:*"
  },
  "keywords": [
    "testurio",
    "mongodb",
    "mongo",
    "adapter",
    "testing",
    "e2e"
  ],
  "author": "",
  "license": "MIT"
}
